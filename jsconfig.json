{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "api/*": ["api/*"],
      "Redux/*": ["redux/*"],
      "utils/*": ["components/utils/*"],
      "public/*": ["public/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "common/*": ["components/common/*"],
      "reducers/*": ["reducers/*"],
      "store/*": ["store/*"],
      "containers/*": ["containers/*"],
      "icons/*": ["public/icons/*"],
      "hooks/*": ["hooks/*"],
      "fetch": ["fetch"]
    }
  }
}
